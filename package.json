{
  "name": "itworx-hub",
  "version": "1.0.0",
  "description": "Enjoy cheap flights, discounted hotels, and exclusive offers to hundreds of popular destinations around the world with tajawal.",
  "keywords": [
    "flights",
    "hotels",
    "discount",
    "offsers",
    "world",
    "travel",
    "cheap"
  ],
  "license": "ISC",
  "author": "Anas Tawfeek",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "node run --env.local",
    "start:dev": "node run --env.development",
    "build": "node run build --env.development",
    "build:prod": "node run build --env.production",
    "build:test": "node run build --env.test",
    "clean": "node run clean",
    "copy": "node run copy --env.development",
    "copy:prod": "node run copy --env.development",
    "slate": "rm -rf node_modules && npm i"
  },
  "babel": {
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy",
      "transform-object-rest-spread",
      "syntax-dynamic-import",
      [
        "transform-regenerator",
        {
          "async": false
        }
      ],
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false,
          "regenerator": true
        }
      ]
    ],
    "presets": [
      "react",
      "latest"
    ]
  },
  "dependencies": {
    "antd": "3.0.0-alpha0",
    "axios": "^0.15.3",
    "babel-polyfill": "^6.20.0",
    "bootstrap": "^4.0.0-alpha.6",
    "classnames": "^2.2.5",
    "classnames-loader": "^2.0.0",
    "compression-webpack-plugin": "^1.0.0",
    "funnies": "^1.2.0",
    "history": "^4.4.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "node-sass": "^3.13.0",
    "prop-types": "^15.5.10",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-helmet": "^5.1.3",
    "react-notification-system": "^0.2.11",
    "react-notification-system-redux": "^1.0.11",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "safe-url-assembler": "^1.3.5",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "cli-spinner": "^0.2.6",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.26.0",
    "del": "^2.2.2",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
